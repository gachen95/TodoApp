{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "Todo application api Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "6.0.1",
    "@nestjs/core": "6.0.1",
    "@nestjs/graphql": "^6.0.1",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/microservices": "6.0.1",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "6.0.1",
    "@nestjs/websockets": "6.0.1",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.4.8",
    "bcrypt": "^3.0.5",
    "csurf": "^1.9.0",
    "dotenv": "^7.0.0",
    "express-rate-limit": "^3.4.0",
    "graphql": "14.1.1",
    "graphql-import": "0.7.1",
    "graphql-subscriptions": "1.0.0",
    "graphql-tools": "4.0.4",
    "graphql-type-json": "0.2.1",
    "helmet": "^3.16.0",
    "jsonwebtoken": "8.5.0",
    "lodash": "4.17.11",
    "luxon": "^1.12.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "prisma-client-lib": "1.28.5",
    "prisma-lib": "0.1.44",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.4.0",
    "typescript": "3.3.3333",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.4.0",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "4.16.1",
    "@types/graphql": "^14.0.7",
    "@types/jest": "24.0.11",
    "@types/node": "^11.11.4",
    "@types/passport-google-oauth2": "^0.1.2",
    "@types/supertest": "^2.0.7",
    "jest": "24.5.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "supertest": "4.0.2",
    "ts-jest": "24.0.0",
    "ts-node": "8.0.3",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.14.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
